; deselect tool
T-1

; lower plate
G1 Z290 F5000

; open bellow
G1 B355 F1000

; turn on cooling fan
M106 F0 S255

; wait cooling
M140 S0
while heat.heaters[0].current > 40.0
	G4 S5

; turn off cooling fan
M106 F0 S0

; unload print
G1 U320 F1000

; home unloader
G28 U0

; close bellow
G1 B0 F1000

; plate in place
G1 Z285 F5000

set global.queue_pos = global.queue_pos + 1
M98 P"/gcodes/queue/inner/start.g"
